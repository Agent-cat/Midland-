{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "JWT_SECRET": "@jwt_secret",
      "MONGO_URI": "@mongo_uri",
      "PORT": "@port",
      "NODE_ENV": "production"
    }
}